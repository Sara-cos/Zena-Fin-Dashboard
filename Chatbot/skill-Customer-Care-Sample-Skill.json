    {
      "type": "standard",
      "title": "FIP&FIU",
      "output": {
        "generic": [
          {
            "values": [
              {
                "text": "An FIP, is a financial information provider—like a bank or an insurance company—that is able to share financial data around a user of their product or service.\n\nAn FIU, or financial information user, is an entity that is requesting for data from an FIP—for their customer—for purposes like verification or enabling financial products for users."
              }
            ],
            "response_type": "text",
            "selection_policy": "sequential"
          }
        ]
      },
      "conditions": "#FIU",
